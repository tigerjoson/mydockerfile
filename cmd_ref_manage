#before use most strart container( mygnscontainer)
docker start mygnscontainer
#2. use 
 ##sigin with root (bash is most)
 docker exec -it -u root mygnscontainer bash
 ##sigin
docker exec -it mygnscontainer bash

#just use 
docker exec -it mygnscontainer pwd


#if volume not yet
#set volume(myvolume) with running container(myjavacontainer) , imagename=eclipse-temurin:latest
docker run  --name=myjavacontainer  --volume myvolume:/javacontainer -dit eclipse-temurin:latest  bash

#1.build  image with "gns3config" file 
#2.set imagename = gns 
#3.  “.” =position of Dockerfile ，or chage to absulute path
#make an container naming mygns ,use image(imagename)
# gns3config =>dockerconfigfile
# set image name=mygns
docker build  -f gns3config -t mygns .
#docker run --name=mygns -it imagename
# using image(gns) ,build container(set name=mygns) 
#-it =>tty
docker run --name=mygnscontainer -it mygns whoami
#docker exec containername(already exits) -it bash
docker exec  mygnscontainer -it   bash
